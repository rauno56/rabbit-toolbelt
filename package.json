{
  "name": "rabbit-toolbelt",
  "version": "1.15.0",
  "type": "module",
  "description": "Validator for the definition files of RabbitMQ",
  "main": "src/validate.js",
  "bin": "cli.js",
  "scripts": {
    "lint": "eslint .",
    "test": "RTB_PATTERN_USERS='^(?!uu)' RTB_PATTERN='^(?!xx)' tap --allow-incomplete-coverage ./test/*.js"
  },
  "keywords": [
    "rabbit",
    "RabbitMQ",
    "amqp",
    "validation",
    "validator",
    "definitions.json",
    "definition",
    "configuration",
    "config"
  ],
  "author": "Rauno Viskus",
  "license": "MIT",
  "engines": {
    "node": ">=22.4"
  },
  "dependencies": {
    "superstruct": "^2.0.2"
  },
  "devDependencies": {
    "eslint": "^9.14.0",
    "tap": "^21.0.1"
  }
}
